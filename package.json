{
  "name": "npm-dependency-analyzer",
  "version": "0.1.0",
  "description": "Plugin to validate dependencies, concerning their license and vulnerabities",
  "main": "index.js",
  "bin": {
    "npm-dependency-analyzer": "./bin/npm-dependency-analyzer.js"
  },
  "scripts": {
    "prebuild": "npm install",
    "build": "npm run lint && npm test",
    "lint": "eslint .",
    "test": "node ./bin/npm-dependency-analyzer.js"
  },
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=2.0.0"
  },
  "keywords": [
    "CLI",
    "vulnerabilities",
    "licenses",
    "npm",
    "build-plugin"
  ],
  "dependencies": {
    "async": "^2.6.0",
    "commander": "^2.15.1",
    "executive": "^1.5.13",
    "nsp": "3.2.1",
    "yargs": "11.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^5.0.4",
    "rimraf": "^2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pt-osda/npm-dependency-analyzer.git"
  },
  "author": {
    "name": "Tiago Lopes",
    "email": "tiago.m_lopes@hotmail.com"
  },
  "license": "BSD-3-Clause"
}
